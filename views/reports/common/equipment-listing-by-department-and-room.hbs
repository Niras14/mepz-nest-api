<html lang='en'>
<head>
  <meta charset='utf-8' />
  <meta name='viewport' content='width=device-width, initial-scale=1' />
  <title>{{title}}</title>
  <link href='https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/css/bootstrap.min.css' rel='stylesheet'
    integrity='sha384-rbsA2VBKQhggwzxH7pPCaAqO46MgnOM80zW1RWuH61DGLwZJEdK2Kadq2F9CUG65' crossorigin='anonymous' />
  <style>
    .page-break {
      page-break-before: always;
    }
  </style>
</head>
<body>
  <div class='container-fluid mb-5 mt-0'>
    <div class='row d-flex align-items-baseline'>
      <div class='col-xl-12'>
        {{!-- {{json departments}} --}}
        {{#each rooms}}
          {{#if ../pagewise}}{{#index_gt @index}}<div class="page-break"></div> {{/index_gt}}{{/if}}
          {{#if (gt data.length 0)}}
          <p style='color: #242729;font-size: 16px; margin-bottom: 5px;'><b>Department:</b> <span>{{deapartcode}}</span> <span
            style="margin-left:30px;">{{deapartname}}</span></p>
          <p style='color: #242729;font-size: 16px; margin-bottom: 5px;'><b>Rooms:</b> <span>{{roomcode}}</span> <span
              style="margin-left:30px;">{{roomname}} </span></p>
          <table class='table table-bordered' style="font-size: 15px; margin-bottom:20px;">
            <thead class='table-light'>
              <tr>
                <th style="padding-top:6px;padding-bottom:6px; color: #242729;">Equipment Code</th>
                <th style="padding-top:6px;padding-bottom:6px; color: #242729;">Equipment Name</th>
                <th style="padding-top:6px;padding-bottom:6px; color: #242729;">Qty</th>
                <th style="padding-top:6px;padding-bottom:6px; color: #242729;">Group</th>
                <th style="padding-top:6px;padding-bottom:6px; color: #242729;">Remarks</th>
                <th style="padding-top:6px;padding-bottom:6px; color: #242729;">YN</th>
                <th style="padding-top:6px;padding-bottom:6px; color: #242729;">Client Remarks</th>
              </tr>
            </thead>
            <tbody>
            {{#each data}}
              <tr>
                <td style="padding-top:6px;padding-bottom:6px">{{code}}</td>
                <td style="padding-top:6px;padding-bottom:6px">{{name}}</td>
                <td style="padding-top:6px;padding-bottom:6px">{{qty}}</td>
                <td style="padding-top:6px;padding-bottom:6px">{{group}}</td>
                <td style="padding-top:6px;padding-bottom:6px"></td>
                <td style="padding-top:6px;padding-bottom:6px"></td>
                <td style="padding-top:6px;padding-bottom:6px"></td>
              </tr>
            {{/each}}
            <tr style="border:0;">
              <td style="border:0;"></td>
              <td style="border-color: #dfe0e1; border-bottom: 1px solid #dfe0e1; padding-top:6px;padding-bottom:6px">Sub Total</td>
              <td style="border-color: #dfe0e1; border-bottom: 1px solid #dfe0e1; padding-top:6px;padding-bottom:6px">{{calculateSumOfQty data}}</td>
              <td style="border:0;"></td>
              <td style="border:0;"></td>
              <td style="border:0;"></td>
              <td style="border:0;"></td>
            </tr>
            </tbody>
          </table>

          {{#if ../w_sign}}
            <table class='table' style="font-size: 15px; margin-bottom:20px;">
              <tbody>
                <tr>
                  <td style="padding-top:6px;padding-bottom:6px" d>Received by:</td>
                  <td style="padding-top:6px;padding-bottom:6px"></td>
                  <td style="padding-top:6px;padding-bottom:6px">MNE SOLUTIONS: </td>
                </tr>
                <tr>
                  <td style="padding-top:6px;padding-bottom:6px;border: 0px;" d>Full Name</td>
                  <td style="padding-top:6px;padding-bottom:6px;border: 0px;">Full Name</td>
                  <td style="padding-top:6px;padding-bottom:6px;border: 0px;">Full Name</td>
                </tr>
                <tr>
                  <td style="padding-top:6px;padding-bottom:6px;border: 0px;">Designation</td>
                  <td style="padding-top:6px;padding-bottom:6px;border: 0px;">Designation</td>
                  <td style="padding-top:6px;padding-bottom:6px;border: 0px;">Designation</td>
                </tr>
                <tr>
                  <td style="padding-top:6px;padding-bottom:6px;border: 0px;">Stamp</td>
                  <td style="padding-top:6px;padding-bottom:6px;border: 0px;">Stamp</td>
                  <td style="padding-top:6px;padding-bottom:6px;border: 0px;">Stamp</td>
                </tr>
                <tr>
                  <td style="padding-top:6px;padding-bottom:6px;border: 0px;">Date</td>
                  <td style="padding-top:6px;padding-bottom:6px;border: 0px;">Date</td>
                  <td style="padding-top:6px;padding-bottom:6px;border: 0px;">Date</td>
                </tr>
              </tbody>
            </table>
          {{/if}}
          {{/if}}
        {{/each}}
        <p style="text-align:right; margin-bottom:0px; color: #242729;"><b>Total Equipment : {{ calculateSumOfTotalEquipmentListingDept rooms }}</b></p>
        {{#if w_sign}}
          <table class='table' style="font-size: 15px; margin-bottom:20px;">
            <tbody>
              <tr>
                <td style="padding-top:6px;padding-bottom:6px" d>Received by:</td>
                <td style="padding-top:6px;padding-bottom:6px"></td>
                <td style="padding-top:6px;padding-bottom:6px">MNE SOLUTIONS: </td>
              </tr>
              <tr>
                <td style="padding-top:6px;padding-bottom:6px;border: 0px;" d>Full Name</td>
                <td style="padding-top:6px;padding-bottom:6px;border: 0px;">Full Name</td>
                <td style="padding-top:6px;padding-bottom:6px;border: 0px;">Full Name</td>
              </tr>
              <tr>
                <td style="padding-top:6px;padding-bottom:6px;border: 0px;">Designation</td>
                <td style="padding-top:6px;padding-bottom:6px;border: 0px;">Designation</td>
                <td style="padding-top:6px;padding-bottom:6px;border: 0px;">Designation</td>
              </tr>
              <tr>
                <td style="padding-top:6px;padding-bottom:6px;border: 0px;">Stamp</td>
                <td style="padding-top:6px;padding-bottom:6px;border: 0px;">Stamp</td>
                <td style="padding-top:6px;padding-bottom:6px;border: 0px;">Stamp</td>
              </tr>
              <tr>
                <td style="padding-top:6px;padding-bottom:6px;border: 0px;">Date</td>
                <td style="padding-top:6px;padding-bottom:6px;border: 0px;">Date</td>
                <td style="padding-top:6px;padding-bottom:6px;border: 0px;">Date</td>
              </tr>
            </tbody>
          </table>
        {{/if}}
      </div>
    </div>
  </div>
</body>
</html>