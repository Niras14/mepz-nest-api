<html lang='en'>
<head>
  <meta charset='utf-8' />
  <meta name='viewport' content='width=device-width, initial-scale=1' />
  <title>{{title}}</title>
  <link href='https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/css/bootstrap.min.css' rel='stylesheet'
    integrity='sha384-rbsA2VBKQhggwzxH7pPCaAqO46MgnOM80zW1RWuH61DGLwZJEdK2Kadq2F9CUG65' crossorigin='anonymous' />
  <style>
    .page-break {
      page-break-before: always;
    }
  </style>
</head>
<body>
  <div class='container-fluid mb-5 mt-0'>
    <div class='row d-flex align-items-baseline'>
      <div class='col-xl-12'>
        {{#each equipments as |EquipmentItemlist key1|}}
          <table class='table table-bordered' style="font-size: 15px; margin-bottom:20px;">
            <thead class='table-light'>
              <tr>
                <th style="padding-top:6px;padding-bottom:6px; color: #242729;">Equipment Code</th>
                <th style="padding-top:6px;padding-bottom:6px; color: #242729;">Equipment Name</th>
                <th style="padding-top:6px;padding-bottom:6px; color: #242729;">Prev Qty</th>
                <th style="padding-top:6px;padding-bottom:6px; color: #242729;">Rev Qty</th>
                <th style="padding-top:6px;padding-bottom:6px; color: #242729;">Diff Qty</th>
                 <th style="padding-top:6px;padding-bottom:6px; color: #242729;">Prev Price</th>
                <th style="padding-top:6px;padding-bottom:6px; color: #242729;">Total Price</th>
                <th style="padding-top:6px;padding-bottom:6px; color: #242729;">Rev Price</th>
                <th style="padding-top:6px;padding-bottom:6px; color: #242729;">Total Price</th>
                <th style="padding-top:6px;padding-bottom:6px; color: #242729;">Diff Price</th>
               {{!-- <th style="padding-top:6px;padding-bottom:6px; color: #242729;">Group</th>
                <th style="padding-top:6px;padding-bottom:6px; color: #242729;">Remarks      </th> --}}
              </tr>
            </thead>
            <tbody>
              {{!-- <tr>
                <td colspan="6"style="padding-top:6px;padding-bottom:6px;"> {{#value_undefine key1}}{{/value_undefine}} </td>
              </tr> --}}
                {{#each EquipmentItemlist as |EquipmentItem key2|}}
              {{#each EquipmentItem}}
                <tr>
                  <td style="padding-top:6px;padding-bottom:6px;">{{code}}</td>
                  <td style="padding-top:6px;padding-bottom:6px;">{{name}}</td>
                  <td style="padding-top:6px;padding-bottom:6px;">{{qty}}</td>
                  <td style="padding-top:6px;padding-bottom:6px;">{{qty_rev}}</td>
                  <td style="padding-top:6px;padding-bottom:6px;"> {{differ_cal qty qty_rev }} </td>
                  <td style="padding-top:6px;padding-bottom:6px;">{{cost}}</td>
                  <td style="padding-top:6px;padding-bottom:6px">{{multiply qty cost}} </td>
                  <td style="padding-top:6px;padding-bottom:6px;">{{cost_rev}}</td>
                  <td style="padding-top:6px;padding-bottom:6px">{{multiply qty_rev cost_rev}} </td>
                  <td style="padding-top:6px;padding-bottom:6px">{{differnce_price EquipmentItem}} </td>
                  <td style="padding-top:6px;padding-bottom:6px;">{{group}}</td>
                  <td style="padding-top:6px;padding-bottom:6px;">{{remarks}}</td>
                </tr>
              {{/each}}
              <tr style="border: 0;">
                <td style="padding-top:6px;padding-bottom:6px; border: 0;"></td>
                <td style="padding-top:6px;padding-bottom:6px;border-color: #dfe0e1; border-bottom: 1px solid #dfe0e1;">Total {{#value_undefine @key2}}{{key2}}{{/value_undefine}}</td>
                <td style="padding-top:6px;padding-bottom:6px;border-color: #dfe0e1; border-bottom: 1px solid #dfe0e1;">{{sum EquipmentItem "qty"}}</td>
                  <td style="border-color: #dfe0e1; border-bottom: 1px solid #dfe0e1; padding-top:6px;padding-bottom:6px">{{sum EquipmentItem "qty_rev"}} </td>
               <td style="border-color: #dfe0e1; border-bottom: 1px solid #dfe0e1; padding-top:6px;padding-bottom:6px">{{total_difernce EquipmentItem "qty" "qty_rev" }}</td>
                <td style="border-color: #dfe0e1; border-bottom: 1px solid #dfe0e1; padding-top:6px;padding-bottom:6px"></td>
                <td style="border-color: #dfe0e1; border-bottom: 1px solid #dfe0e1; padding-top:6px;padding-bottom:6px">{{calculateSumOfTotal EquipmentItem }}</td>
                <td style="border-color: #dfe0e1; border-bottom: 1px solid #dfe0e1; padding-top:6px;padding-bottom:6px"></td>
                <td style="border-color: #dfe0e1; border-bottom: 1px solid #dfe0e1; padding-top:6px;padding-bottom:6px">{{calculateSumOfTotal_rev EquipmentItem }}</td>
                <td style="border-color: #dfe0e1; border-bottom: 1px solid #dfe0e1; padding-top:6px;padding-bottom:6px">{{total_price_difernce EquipmentItem }}  </td>
              </tr>
                {{/each}}
              {{#if ../pagewise}}{{#index_gt @index}}<div class="page-break"> </div> {{/index_gt}} {{/if}}
            </tbody>
          </table>
        {{/each}}
        <p style="text-align:right; margin-bottom:0px; color: #242729;"><b>Total Equipment : {{ calculateSumOfTotalEquipmentListingBQbyGroup equipments }}</b></p>
           <p style="text-align:right; margin-bottom:0px; color: #242729;"><b>Total : 
          {{ calculateSumOfTotalPriceEquipmentListingBQbyGroup equipments }}</b></p
      </div>
    </div>
  </div>
</body>
</html>